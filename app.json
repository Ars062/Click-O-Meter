{
  "expo": {
    "name": "Click-O-Meter",
    "slug": "Click-O-Meter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/click-o-meter.jpg",
    "scheme": "clickometer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ars062.ClickOMeter",
      "versionCode": 1,
      "permissions": [
        "VIBRATE",
        "INTERNET"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/click-o-meter.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/click-o-meter.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/click-o-meter.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50a0c18e-5293-4ab4-9610-3e8f775bcf81"
      }
    }
  }
}
